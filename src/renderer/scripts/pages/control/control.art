<div class="{{ styles.component }}">
    <p class="{{@ styles.time }}">00:00</p>

    <div class="{{ styles.option }}">
        <p>工作时间：
            {{@ inputs.workTime }}
        </p>
        <p>休息时间：
            {{@ inputs.restTime }}
        </p>
    </div>
    
    <div class="{{ styles.button }}">
        {{@ buttons.work }}
        {{@ buttons.rest }}
        {{@ buttons.cancel }}
        {{@ buttons.now }}
    </div>
</div>