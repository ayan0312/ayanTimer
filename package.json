{
  "name": "ayan-timer",
  "version": "0.2.0",
  "author": "ayan0312 <ayan0312000@outlook.com>",
  "description": "a tomato timer",
  "main": "./dist/electron-build/main.js",
  "scripts": {
    "env:prod":"cross-env NODE_ENV=production",
    "env:dev":"cross-env NODE_ENV=development",
    "web:build": "npm run env:prod && webpack --config build/web/prod.config.js --progress --display-error-details --colors",
    "web:dev": "npm run env:dev && webpack-dev-server --config build/web/dev.config.js --watch --colors",
    "electron:dev": "npm run env:dev && webpack --config build/electron/electronConfigMain.babel.js && electron ./dist/electron-build/main.js",
    "electron:build": "npm run env:prod && electron-builder build",
    "test": "mocha"
  },
  "build": {
    "productName": "ayanTimer",
    "appId": "com.example.app",
    "copyright": "ayan",
    "directories": {
      "output": "dist/electron"
    },
    "win": {
      "icon": "build/icons/ayanTimer-256x256.ico",
      "target": [
        {
          "target": "nsis"
        }
      ]
    }
  },
  "nsis": {
    "oneClick": false,
    "allowElevation": true,
    "allowToChangeInstallationDirectory": true,
    "installerIcon": "build/icons/ayanTimer-256x256.ico",
    "uninstallerIcon": "build/icons/uninstall-256x256.ico",
    "installerHeaderIcon": "build/icons/ayanTimer-256x256.ico",
    "createDesktopShortcut": true,
    "createStartMenuShortcut": true,
    "shortcutName": "ayanTimer"
  },
  "files": [
    "dist/build/**/*"
  ],
  "bugs": {
    "url": "https://github.com/ayan0312/ayanTimer/issues"
  },
  "repository": "https://github.com/ayan0312/ayanTimer.git",
  "license": "MIT",
  "dependencies": {
    "asar": "^2.0.1",
    "balloon-css": "^0.5.0",
    "node-gyp": "^4.0.0",
    "promise-polyfill": "^8.1.0",
    "smoothscroll": "^0.4.0"
  },
  "devDependencies": {
    "art-template": "^4.13.2",
    "art-template-loader": "^1.4.3",
    "autoprefixer": "^9.5.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "electron": "^5.0.2",
    "electron-builder": "^20.41.0",
    "file-loader": "^3.0.1",
    "mini-css-extract-plugin": "^0.7.0",
    "mocha": "^6.1.4",
    "mochawesome": "^3.1.2",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1",
    "webpack-merge": "^4.2.1"
  }
}
