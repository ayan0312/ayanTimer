{
  "name": "pomodoro",
  "version": "0.2.1",
  "description": "pomodoro",
  "main": "dist/main.js",
  "scripts": {
    "build": "npm-run-all build:*",
    "build:main": "cross-env NODE_ENV=production webpack --config webpack/webpackConfigMain.babel.js",
    "build:renderer": "cross-env NODE_ENV=production webpack --config webpack/webpackConfigRenderer.babel.js",
    "dev": "cross-env NODE_ENV=development babel-node ./webpack/devRunner.babel.js",
    "release": "npm-run-all clean:* && npm-run-all release:*",
    "release:win": "npm run build && electron-builder build --win --publish always",
    "ele":"cross-env NODE_ENV=development electron ./dist/main.js",
    "clean:dist": "rimraf ./dist",
    "clean:release": "rimraf ./release",
    "test:unit": "karma start",
    "test": "npm run lint && npm run build && npm run test:unit",
    "lint": "npm-run-all lint:*",
    "lint:ts": "tslint \"src/**/*.{ts,tsx}\" -p tsconfig.json",
    "lint:prettier": "prettier \"src/**/*.{js,jsx,ts,tsx,css}\" --list-different",
    "fix": "npm-run-all fix:*",
    "fix:ts": "npm run lint:ts -- --fix",
    "fix:prettier": "prettier \"src/**/*.{js,jsx,ts,tsx,css}\" --write",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "linters": {
      "{src,test}/**/*.{js,ts,tsx}": [
        "prettier",
        "tslint -p tsconfig.json"
      ]
    }
  },
  "build": {
    "productName": "Pomodoro",
    "appId": "com.github.ayan0312.pomodoro",
    "asar": true,
    "directories": {
      "output": "release"
    },
    "files": [
      "dist",
      "package.json"
    ],
    "win": {
      "icon": "resources/icons/icon.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "ia32",
            "x64"
          ]
        }
      ]
    },
    "nsis": {
      "perMachine": false,
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "include": "resources/windows/installer.nsh"
    }
  },
  "private": true,
  "pre-push": [
    "validate"
  ],
  "husky": {
    "hooks": {
      "post-commit": "git reset",
      "pre-commit": "lint-staged"
    }
  },
  "author": {
    "name": "ayan0312",
    "email": "ayan0312@outlook.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ayan0312/pomodoro"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "@types/chai": "^4.1.7",
    "@types/classnames": "^2.2.8",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.0.8",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/react-hot-loader": "^4.1.0",
    "@types/react-redux": "^7.1.0",
    "autoprefixer": "^9.6.0",
    "cfonts": "^2.4.3",
    "chai": "^4.2.0",
    "chalk": "^2.4.2",
    "classnames": "^2.2.6",
    "core-js": "^3.1.4",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "delay": "^4.3.0",
    "electron": "^5.0.4",
    "electron-builder": "^20.43.0",
    "file-loader": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "2.4.1",
    "karma": "4.1.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-typescript": "^4.1.0",
    "lint-staged": "^8.2.0",
    "mini-css-extract-plugin": "^0.7.0",
    "mocha": "^6.1.4",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "prettier": "1.18.2",
    "react-hot-loader": "^4.11.1",
    "rimraf": "2.6.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "svg-inline-loader": "^0.8.0",
    "ts-loader": "^6.0.2",
    "tslint": "^5.17.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.5.1",
    "url-loader": "^2.0.0",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1",
    "webpack-hot-middleware": "^2.24.0"
  },
  "dependencies": {
    "antd": "^3.19.5",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "redux": "^4.0.1"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  }
}
